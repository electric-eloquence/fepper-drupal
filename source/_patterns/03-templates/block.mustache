{{{ twig_docblock }}}
<!-- Begin block -->
<div {{{ block_attributes }}}>

  {{{ title_prefix }}}
  {{# title }}
    <h2 {{{ title_attributes }}}><a href="#">{{{ title }}}</a></h2>
  {{/ title }}
  {{{ title_suffix }}}

  {{# content? }}

    {{! BEGIN HEADER }}

    {{# compounds-logo? }}
      {{> 03-templates/_block--system-branding-block }}
    {{/ compounds-logo? }}
    {{# compounds-logo~~pages-new-install? }}
      {{> 03-templates/_block--system-branding-block(
        "site_name_or_site_slogan?": true,
        "site_name?": true,
        "page_title_or_not_homepage?": true,
        "compounds-logo?": true
      ) }}
    {{/ compounds-logo~~pages-new-install? }}
    {{# compounds-search? }}
      {{> 01-compounds/02-forms/00-search }}
    {{/ compounds-search? }}

    {{! END HEADER }}


    {{! BEGIN BREADCRUMBS }}

    {{# components-breadcrumbs? }}
      {{> 03-templates/_block--system-menu-block(
        heading_id: 'system-breadcrumb',
        title_attributes: ' class="visually-hidden" id="system-breadcrumb"',
        title: 'Breadcrumb',
        "components-breadcrumbs?": true
      )}}
    {{/ components-breadcrumbs? }}

    {{! END BREADCRUMBS }}


    {{! BEGIN HOMEPAGE }}

    {{# templates-container~~pages-homepage__hero? }}
      {{> 03-templates/container("templates-views-view~~pages-homepage__hero?": true) }}
    {{/ templates-container~~pages-homepage__hero? }}
    {{# templates-container~~pages-homepage__hoagies? }}
      {{> 03-templates/container("templates-views-view~~pages-homepage__hoagies?": true) }}
    {{/ templates-container~~pages-homepage__hoagies? }}

    {{! END HOMEPAGE }}


    {{! BEGIN BLOG }}

    {{# templates-container~~pages-blog? }}
      {{> 03-templates/container("templates-views-view~~pages-blog?": true) }}
    {{/ templates-container~~pages-blog? }}

    {{! END BLOG }}


    {{! BEGIN ARTICLE }}

    {{# templates-node~~pages-article? }}
      {{> 03-templates/node(
        node_attributes: ' role="article" typeof="schema:Article"',
        "view_mode_full?": true,
        "components-article?": true
      ) }}
    {{/ templates-node~~pages-article? }}

    {{! END ARTICLE }}


    {{! BEGIN NEW INSTALL }}

    {{# elements-heading? }}
      {{> 00-elements/00-text/01-heading(
        heading_tag: 'h1',
        heading_attributes: ' class="js-quickedit-page-title"',
        heading_text: 'Welcome to Fepper Theme'
      ) }}
    {{/ elements-heading? }}
    {{# views-view~~pages-new-install? }}
      {{> 03-templates/views-view(
        views-view_attributes: ' class="view-id-frontpage view-display-id-page_1"',
        "pages-new-install?": true
      ) }}
    {{/ views-view~~pages-new-install? }}
    {{# compounds-powered-by? }}
      {{> 01-compounds/00-text/06-powered-by }}
    {{/ compounds-powered-by? }}

    {{! END NEW INSTALL }}


    {{! BEGIN SIDEBAR2 VIEWS }}

    {{# templates-views-view~~templates-block--sidebar2__posts? }}
      {{> 03-templates/views-view(
        views-view_attributes: ' class="view-id-content_recent view-display-id-block_1"',
        "templates-views-view-list~~templates-block--sidebar2__posts?": true
      ) }}
    {{/ templates-views-view~~templates-block--sidebar2__posts? }}
    {{# templates-views-view~~templates-block--sidebar2__comments? }}
      {{> 03-templates/views-view(
        views-view_attributes: ' class="view-id-comments_recent view-display-id-block_1"',
        "templates-views-view-list~~templates-block--sidebar2__comments?": true
      ) }}
    {{/ templates-views-view~~templates-block--sidebar2__comments? }}

    {{! END SIDEBAR2 VIEWS }}


    {{! BEGIN FOOTER }}

    {{# components-footer? }}
      {{> 01-compounds/00-text/05-copyright }}
    {{/ components-footer? }}

    {{! END FOOTER }}


    {{{ fpo_template_content }}}

  {{/ content? }}

</div>
<!-- End block -->
