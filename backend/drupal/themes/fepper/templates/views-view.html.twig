{% set classes = [dom_id ? 'js-view-dom-id-' ~ dom_id,] %}
<!-- Begin views-view -->
<div{{ attributes.addClass(classes) }}>
  {{ title_prefix }}
  {% if title %}
    <h1>{{ title }}</h1>
  {% endif %}
  {{ title_suffix }}

  {% if header %}
    <header>
      {{ header }}
    </header>
  {% endif %}

  {{ exposed }}
  {{ attachment_before }}


  {{ rows }}
  {{ empty }}
  {{ pager }}

  {{ attachment_after }}
  {{ more }}

  {% if footer %}
    <footer>
      {{ footer }}
    </footer>
  {% endif %}

  {{ feed_icons }}
</div>
<!-- End views-view -->
