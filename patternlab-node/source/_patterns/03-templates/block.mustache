<!-- Begin block -->
<div{{{ content_block_attributes }}}>
  {{ title_prefix }}
  {{# label? }}
    <h2{{{ title_attributes }}}>{{ label }}</h2>
  {{/ label? }}
  {{ title_suffix }}
  {{# content? }}
    {{# templates-block? }}
      {{{ fpo_template_content }}}
    {{/ templates-block? }}
    {{# atoms-logo? }}
      {{> 00-atoms/03-images/00-logo }}
    {{/ atoms-logo? }}
    {{# molecules-search? }}
      {{> 01-molecules/04-forms/00-search }}
    {{/ molecules-search? }}
    {{# templates-node--article? }}
      {{> 03-templates/node(
        'organisms-article?': true,
        'view_mode_full?': true
      ) }}
    {{/ templates-node--article? }}
    {{# organisms-related-posts? }}
      {{> 02-organisms/04-sections/02-related-posts }}
    {{/ organisms-related-posts? }}
    {{# organisms-recent-tweets? }}
      {{> 02-organisms/04-sections/01-recent-tweets }}
    {{/ organisms-recent-tweets? }}
    {{# organisms-footer? }}
      {{> 02-organisms/00-global/01-footer }}
    {{/ organisms-footer? }}
  {{/ content? }}
</div>
<!-- End block -->
