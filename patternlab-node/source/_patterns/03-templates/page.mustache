<!-- Begin page -->
<div{{{ page_attributes }}}>

  <header class="header cf" role="banner">
    {{> 03-templates/region("templates-block--header?": true) }}
  </header>

  {{ page.primary_menu }}
  {{ page.secondary_menu }}

  {{ page.breadcrumb }}

  {{# emergency }}
    {{> 01-compounds/07-messaging/00-alert }}
  {{/ emergency }}

  {{ page.help }}

  <main role="main">
    <a id="main-content" tabindex="-1"></a>

    <div class="layout-content">
      {{# templates-page? }}
        {{{ fpo_template_content }}}
      {{/ templates-page? }}
      {{# templates-region--homepage? }}
        {{> 03-templates/region('templates-block--main--homepage?': true) }}
      {{/ templates-region--homepage? }}
      {{# templates-region--blog? }}
        {{> 03-templates/region("templates-block--main--blog?": true) }}
      {{/ templates-region--blog? }}
      {{# templates-region--article? }}
        {{> 03-templates/region("templates-block--main--article?": true) }}
      {{/ templates-region--article? }}
    </div>

    {{# sidebar_first? }}
      <aside class="layout-sidebar-first" role="complementary">
        {{# templates-region--homepage? }}
          {{> 03-templates/region('templates-block--sidebar1--homepage?': true) }}
        {{/ templates-region--homepage? }}
      </aside>
    {{/ sidebar_first? }}

    {{# sidebar_second? }}
      <aside class="layout-sidebar-second" role="complementary">
        {{# templates-region--article? }}
          {{> 03-templates/region("templates-block--sidebar2--article?": true) }}
        {{/ templates-region--article? }}
        {{> 03-templates/region("templates-block--sidebar2?": true) }}
      </aside>
    {{/ sidebar_second? }}

  </main>

  {{# footer? }}
    <footer role="contentinfo">
      {{> 03-templates/region('templates-block--footer?': true) }}
    </footer>
  {{/ footer? }}

</div>
<!-- End page -->
