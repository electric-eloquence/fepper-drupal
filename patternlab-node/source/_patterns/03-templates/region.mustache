<!-- Begin region -->
{{# content? }}
  <div{{ attributes }}>
    {{# templates-region? }}
      {{{ fpo_template_content }}}
    {{/ templates-region? }}
    {{# templates-block--header? }}
      {{> 03-templates/block(
        'organisms-header?': true,
        content_block_attributes: ' id="block-fepper-branding" class="contextual-region"'
      ) }}
    {{/ templates-block--header? }}
    {{# templates-block--main? }}
      {{> 03-templates/block(
        'templates-node--article?': true,
        content_block_attributes: ' id="block-fepper-content"'
      ) }}
    {{/ templates-block--main? }}
    {{# templates-block--sidebar2? }}
      {{> 03-templates/block(
        'organisms-sidebar2?': true,
        content_block_attributes: ' class="views-element-container contextual-region" id="block-views-block-comments-recent-block-1"'
      ) }}
    {{/ templates-block--sidebar2? }}
  </div>
{{/ content? }}
<!-- End region -->
